{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "@ant-design/pro-layout": "^6.5.0",
    "@scena/guides": "^0.17.0",
    "antd": "^4.16.11",
    "classnames": "^2.3.1",
    "lib-flexible": "^0.3.2",
    "lodash": "^4.17.21",
    "react": "17.x",
    "react-dom": "17.x",
    "react-input-color": "^4.0.0",
    "umi": "^3.5.17",
    "zarm": "^2.9.8"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.17",
    "babel-plugin-import": "^1.13.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
